<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Map[]>
<Map
  srs="epsg:4326"
  background-color="#dfd8c9">

<Style name="style">
  <Rule>
    <RasterSymbolizer />
  </Rule>
</Style>
<Layer name="layer"
  srs="epsg:4326">
    <StyleName>style</StyleName>
    <Datasource>
       <Parameter name="file">./valid.geotiff.tif</Parameter>
       <Parameter name="type">raster</Parameter>
    </Datasource>
  </Layer>

</Map>
